{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "types": [
      "node"
    ],
    "paths": {
      "@store/*": ["src/app/store/*"],
      "@core/*": ["src/app/core/*"],
      "@shared/*": ["src/app/shared/*"],
      "@env": ["src/environments/environment.ts"]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
